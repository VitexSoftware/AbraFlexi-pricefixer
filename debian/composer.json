{
    "name": "vitexsoftware/abraflexi-pricefixer",
    "description": "Product price updater for AbraFlexi",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
        }
    },
    "authors": [
        {
            "name": "Cybervitexus",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "config": {
        "vendor-dir": "/var/lib/abraflexi-pricefixer"
    },
    "require": {
        "deb/abraflexi": "*"
    },
    "autoload": {
        "psr-4": {
            "AbraFlexi\\PriceFix\\": "/usr/lib/abraflexi-pricefixer/PriceFix/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "/usr/share/php/EaseCore"
        },
        {
            "type": "path",
            "url": "/usr/share/php/AbraFlexi"
        }
    ]
    "minimum-stability": "dev"
}
